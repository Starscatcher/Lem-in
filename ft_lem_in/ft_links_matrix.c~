#include "lem_in.h"

int	ft_find_ind(char *name, t_room *room)
{
	int i;

	i = 0;
	while (room && ft_strcmp(name, room->name))
	{
		room = room->next;
		i++;
	}
	if (!room)
		ft_print_error(1, "Not valid link");
	return (i);
}

int	**ft_make_matrix(int **matrix, int len)
{
	int i;
	int j;

	i = 0;
	j = 0;
	while (i != len)
	{
		matrix[i] = (int*)malloc(sizeof(int) * len);
		while (j != len)
		{
			matrix[i][j] = 0;
			j++;
		}
		j = 0;
		i++;
	}
	return (matrix);
}
